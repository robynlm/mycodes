   RowBox[{"And", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"x", "[", "]"}], "|", 
        RowBox[{"y", "[", "]"}], "|", 
        RowBox[{"z", "[", "]"}], "|", 
        RowBox[{"r", "[", "]"}], "|", 
        RowBox[{"\[Theta]", "[", "]"}], "|", 
        RowBox[{"\[Phi]", "[", "]"}]}], ")"}], "\[Element]", "Reals"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"r", "[", "]"}], ">", "0"}], ",", "\[IndentingNewLine]", 

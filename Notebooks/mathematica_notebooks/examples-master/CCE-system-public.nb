        RowBox[{"g", "[", 
         RowBox[{
          RowBox[{"-", "b"}], ",", 
          RowBox[{"-", "A"}]}], "]"}], ",", 
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox[
           RowBox[{"r", "[", "]"}], "2"]}], 

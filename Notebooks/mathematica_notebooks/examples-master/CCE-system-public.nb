  RowBox[{"Join", "[", 
   RowBox[{
    RowBox[{"MakeRule", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"g", "[", 
        RowBox[{
         RowBox[{"-", "A"}], ",", 

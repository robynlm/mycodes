Cell[BoxData[
 RowBox[{"RiccicdsToqRule", "=", 
  RowBox[{"MakeRule", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Riccicds", "[", 

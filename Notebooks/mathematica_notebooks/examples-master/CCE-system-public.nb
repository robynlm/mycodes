        SuperscriptBox[
         RowBox[{"r", "[", "]"}], "2"], 
        RowBox[{"h", "[", 
         RowBox[{
          RowBox[{"-", "A"}], ",", 
          RowBox[{"-", "B"}]}], "]"}]}]}], "}"}], "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"MakeRule", "@", 
     RowBox[{"Evaluate", "@", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"g", "[", 
         RowBox[{
          RowBox[{"-", "A"}], ",", 
          RowBox[{"-", "b"}]}], "]"}], ",", 
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox[
           RowBox[{"r", "[", "]"}], "2"]}], 
         RowBox[{"h", "[", 
          RowBox[{
           RowBox[{"-", "A"}], ",", 
           RowBox[{"-", "B"}]}], "]"}], 
         RowBox[{"U", "[", "B", "]"}], 
         RowBox[{"du", "[", 
          RowBox[{"-", "b"}], "]"}]}]}], "}"}]}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"MakeRule", "@", 
     RowBox[{"Evaluate", "@", 
      RowBox[{"{", 

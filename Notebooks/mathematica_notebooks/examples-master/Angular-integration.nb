Cell["\<\
Now we can make a multi-index object which is a product of any number of n \
tensors.\
\>", "Text"],

Cell["\<\
Sorry that this code looks like it's digging into the xTensor guts -- there \
used to be other stuff in here specific to pN which I've removed. It's easier \
for me to do this than rewrite it.\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DefN", "[", 
    RowBox[{"v_", ",", 
     RowBox[{"M_", "?", "ManifoldQ"}], ",", "pa_"}], "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"tangent", "=", 
       RowBox[{"TangentBundleOfManifold", "[", "M", "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"xTensorQ", "[", "v", "]"}], "^:=", "True"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Dagger", "[", "v", "]"}], "^:=", "v"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"DefInfo", "[", "v", "]"}], "^:=", 
       RowBox[{"{", 
        RowBox[{"\"\<tensor\>\"", ",", "\"\<\>\""}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"DependenciesOfTensor", "[", "v", "]"}], "^:=", 
       RowBox[{"{", "M", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"HostsOf", "[", "v", "]"}], "^:=", 
       RowBox[{"{", 
        RowBox[{"M", ",", "tangent"}], "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"PrintAs", "[", "v", "]"}], "^:=", "pa"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"SlotsOfTensor", "[", "v", "]"}], "^:=", 
       RowBox[{"{", 
        RowBox[{"AnyIndices", "@", "tangent"}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"SymmetryGroupOfTensor", "[", 
        RowBox[{"v", "[", "inds__", "]"}], "]"}], "^:=", 
       RowBox[{"Symmetric", "@", 
        RowBox[{"Range", "@", 
         RowBox[{"Length", "@", 
          RowBox[{"{", "inds", "}"}]}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"TensorID", "[", "v", "]"}], "^:=", 
       RowBox[{"{", "}"}]}], ";"}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]], "Input"],


Cell["Fixing index heights on n, Ns, and NSTF", "Subsubsection"],

Cell["\<\
In general we want all the indices to be in the same position (up). They get \
moved around during canonicalization and other operations. This should change \
them.\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"SeparateMetricNsrule", ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"n", "[", 
       RowBox[{"-", "a_"}], "]"}], "\[RuleDelayed]", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"d", "=", 
          RowBox[{"DummyIn", "[", "TangentM", "]"}]}], "}"}], ",", 
        RowBox[{
         RowBox[{"n", "[", "d", "]"}], 
         RowBox[{"met", "[", 
          RowBox[{
           RowBox[{"-", "d"}], ",", 
           RowBox[{"-", "a"}]}], "]"}]}]}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Ns", "[", 
       RowBox[{"firsts___", ",", 
        RowBox[{"-", "a_"}], ",", "rests___"}], "]"}], "\[RuleDelayed]", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"d", "=", 
          RowBox[{"DummyIn", "[", "TangentM", "]"}]}], "}"}], ",", 
        RowBox[{
         RowBox[{"Ns", "[", 
          RowBox[{"firsts", ",", "d", ",", "rests"}], "]"}], 
         RowBox[{"met", "[", 
          RowBox[{
           RowBox[{"-", "d"}], ",", 
           RowBox[{"-", "a"}]}], "]"}]}]}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"NSTF", "[", 
       RowBox[{"firsts___", ",", 
        RowBox[{"-", "a_"}], ",", "rests___"}], "]"}], "\[RuleDelayed]", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"d", "=", 
          RowBox[{"DummyIn", "[", "TangentM", "]"}]}], "}"}], ",", 
        RowBox[{
         RowBox[{"NSTF", "[", 
          RowBox[{"firsts", ",", "d", ",", "rests"}], "]"}], 
         RowBox[{"met", "[", 
          RowBox[{
           RowBox[{"-", "d"}], ",", 
           RowBox[{"-", "a"}]}], "]"}]}]}], "]"}]}]}], "\[IndentingNewLine]", 
    "}"}]}], ";"}]], "Input"]
}, Closed]],

